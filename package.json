{
  "name": "aitest",
  "version": "1.0.0",
  "description": "Writes your test case for you with AI",
  "bin": {
    "aitest": "bin.mjs"
  },
  "type": "module",
  "files": [
    "dist",
    "bin.mjs",
    "*.md"
  ],
  "durka": {
    "env": "node",
    "commands": {
      "build": "@durka/rollup-cli@<1.0.0",
      "jest": "@durka/test-cli@<1.0.0",
      "eslint": "@durka/lint-cli@<1.0.0"
    }
  },
  "scripts": {
    "test": "durka jest",
    "test:watch": "npm run test -- --watch",
    "lint": "durka eslint --ext .js,.ts ./src",
    "lint:fix": "npm run lint -- --fix",
    "build": "durka build --format es",
    "build:sourcemap": "npm run build -- --sourcemap",
    "build:watch": "npm run build -- -w",
    "release": "durka release",
    "prepare": "durka run prepare && npm run lint && npm run test && npm run build",
    "preinstall": "durka only-allow npm"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.18.0",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Mora <qiuzhongleiabc@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@durka/cli": "latest",
    "@serpent/common-cli": "^3.0.0",
    "@types/node": "^14",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "preset": "./durka/vendors/jest-config"
  },
  "eslintConfig": {
    "extends": "./durka/vendors/eslint-config"
  },
  "eslintIgnore": [
    "dist",
    "durka/*",
    "!durka/config"
  ],
  "repository": {
    "type": "git",
    "repository": "git@github.com:durkajs/example-ts-node-cli.git"
  },
  "commitlint": {
    "extends": [
      "./durka/vendors/commitlint-config"
    ]
  }
}
